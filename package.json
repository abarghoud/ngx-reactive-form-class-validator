{
  "name": "ngx-reactive-form-class-validator",
  "description": "A lightweight library for dynamically validate Angular reactive forms using class-validator library.",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "ng",
    "angular",
    "library",
    "ngx",
    "typescript",
    "reactive-forms",
    "class-validator",
    "dynamic-validator",
    "validation",
    "validator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abarghoud/ngx-reactive-form-class-validator.git"
  },
  "bugs": {
    "url": "https://github.com/abarghoud/ngx-reactive-form-class-validator/issues"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test ngx-reactive-form-class-validator",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:lib-pack": "ng build ngx-reactive-form-class-validator && npm run copy-files && cd dist/libs/ngx-reactive-form-class-validator && npm pack",
    "copy-readme": "copy .\\README.md .\\dist\\libs\\ngx-reactive-form-class-validator",
    "copy-license": "copy .\\LICENSE.txt .\\dist\\libs\\ngx-reactive-form-class-validator",
    "copy-changelog": "copy .\\Changelog.md .\\dist\\libs\\ngx-reactive-form-class-validator",
    "copy-files": "run-s copy-readme copy-license",
    "bump-version:major": "cd libs/ngx-reactive-form-class-validator && npm version major --force",
    "bump-version:minor": "cd libs/ngx-reactive-form-class-validator && npm version minor --force",
    "bump-version:patch": "cd libs/ngx-reactive-form-class-validator && npm version patch --force",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack",
    "pre-publish:major": "run-s bump-version:major build:lib-pack",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack commit-git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@nrwl/angular": "10.3.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2",
    "class-validator": "^0.12.2",
    "cerialize": "^0.1.18"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/eslint-plugin-nx": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "^10.1.0",
    "prettier": "2.0.4",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3"
  }
}
